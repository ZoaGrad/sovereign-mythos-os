intent      = "Sovereign", action, args, [policy] ;
action      = "Infer" | "Train" | "Share" ;
args        = "(" cid:CID { "," cid:CID } ")" ;
policy      = " with " policykv { "," policykv } ;
policykv    = ident "=" value ;
CID         = "ipfs://" , ( ALNUM | ":" )+ ;

